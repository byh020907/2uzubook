
CREATE TABLE `account` (
  `student_id` int(11) NOT NULL,
  `id` varchar(100) NOT NULL,
  `password` varchar(100) NOT NULL,
  `intro` varchar(200) DEFAULT NULL,
  `name` varchar(100) NOT NULL,
  `gender` varchar(7) NOT NULL,
  `major` varchar(20) NOT NULL,
  `email` varchar(50) NOT NULL,
  PRIMARY KEY (`student_id`),
  UNIQUE KEY `id` (`id`),
  UNIQUE KEY `email` (`email`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


CREATE TABLE `resume` (
  number int not null auto_increment,
  student_id int not null,
  name varchar(100) not null,
  content text not null,
  startDate Date null,
  finishDate Date null,
  resume_num int not null,
  keyword_num int not null,
  PRIMARY KEY (`number`),
  CONSTRAINT `fk_resume_account` FOREIGN KEY (`student_id`) REFERENCES `account` (`student_id`),
  CONSTRAINT fk_resume_category FOREIGN KEY (resume_num) REFERENCES resume_category (num),
  CONSTRAINT fk_keyword_category FOREIGN KEY (keyword_num) REFERENCES keyword_category (num)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


  'number' int nit
  `student_id` int(11) NOT NULL,
  `name` varchar(100) NOT NULL,
  `content` text NOT NULL,
  `startDate` date DEFAULT NULL,
  `finishDate` date DEFAULT NULL,
  `resume_category` int(11) NOT NULL,
  `keyword_category` int(11) NOT NULL,
  PRIMARY KEY (`number`),
  CONSTRAINT `fk_resume_account` FOREIGN KEY (`student_id`) REFERENCES `account` (`student_id`),
  CONSTRAINT 'fk_resume_category' FOREIGN KEY ('resume_category') REFERENCES 'resume_category' ('num'),
  CONSTRAINT 'fk_keyword_category' FOREIGN KEY ('keyword_category') REFERENCES 'keyword_category' ('num')
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `resume_category` (
  `num` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(100) NOT NULL,
  PRIMARY KEY (`num`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `keyword_category` (
  `num` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(100) NOT NULL,
  PRIMARY KEY (`num`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `serialkey` (
  `serialkey_num` int(11) NOT NULL AUTO_INCREMENT,
  `serialKey` varchar(500) NOT NULL,
  `keyword_category` int(11) DEFAULT NULL,
  PRIMARY KEY (`serialkey_num`),
  UNIQUE KEY `serialkey` (`serialKey`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;